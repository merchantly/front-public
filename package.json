  "scripts": {
    "deploy-storybook": "storybook-to-ghpages",
    "shell": "node -r @babel/register",
    "legacy_build:snippet": "WIDGETS_URL=../scripts/widgets envify app/scripts/embeddable/snippet.js | uglifyjs -m -c unused,booleans,conditionals,dead_code,if_return,join_vars > build/scripts/snippet.js",
    "legacy_dist:snippet": "WIDGETS_URL=http://assets.kiiiosk.ru/widgets envify app/scripts/embeddable/snippet.js | uglifyjs -m -c unused,booleans,conditionals,dead_code,if_return,join_vars > dist/scripts/snippet.js",
    "test": "yarn test:browser && yarn test:prerender && yarn test:mini_racer",
    "test:browser": "mocha-chrome test/index.html",
    "test:prerender": " mocha --compilers js:@babel/register ./test/test_prerender.js",
    "test:mini_racer": " ./test/mini_racer_test.rb",
    "build": "yarn clean; yarn build:production; yarn build:development",
    "build:production": "NODE_ENV=production webpack-cli --progress --mode production",
    "build:development": "NODE_ENV=development webpack-cli --progress --mode development",
    "profile": "NODE_ENV=production webpack-cli --profile --json > stats.json && webpack-bundle-analyzer ./stats.json",
    "clean": "rm -rf dist/*",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'app/bower_components', 'junction') } catch (e) { }\"",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "build-storybook"
  },
